{
  "name": "ubuntu-piso-wifi",
  "version": "1.0.0",
  "description": "Piso Wifi System for Ubuntu",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "prod": "pm2 start ecosystem.config.js --env production",
    "stop": "pm2 stop ecosystem.config.js",
    "restart": "pm2 restart ecosystem.config.js",
    "monit": "pm2 monit",
    "fix-pm2": "pm2 update",
    "backup": "node src/scripts/create_backup.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.97.0",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^9.0.0",
    "body-parser": "^1.20.2",
    "chart.js": "^4.4.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "onoff": "^6.0.3",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "pm2": "^5.3.0"
  }
}
